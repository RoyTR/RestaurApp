<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="EditTextRestaurAppTheme" parent="android:Widget.EditText">
	  <item name="android:background">@drawable/restaurapptheme_edit_text_holo_light</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="AutoCompleteTextViewRestaurAppTheme" parent="android:Widget.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/restaurapptheme_list_selector_holo_light</item>
	  <item name="android:background">@drawable/restaurapptheme_edit_text_holo_light</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="CheckBoxRestaurAppTheme" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/restaurapptheme_btn_check_holo_light</item>
  </style>

  <style name="RadioButtonRestaurAppTheme" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/restaurapptheme_btn_radio_holo_light</item>
  </style>

  <style name="ButtonRestaurAppTheme" parent="android:Widget.Button">
	  <item name="android:background">#666666</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="ImageButtonRestaurAppTheme" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/restaurapptheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerRestaurAppTheme" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/restaurapptheme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/restaurapptheme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerDropDownItemRestaurAppTheme" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/restaurapptheme_btn_radio_holo_light</item>
  </style>
  <style name="TabRestaurAppTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/restaurapptheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextRestaurAppTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarRestaurAppTheme" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/restaurapptheme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/restaurapptheme_progress_indeterminate_horizontal_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBarRestaurAppTheme" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/restaurapptheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/restaurapptheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:minHeight">13dip</item>
      <item name="android:maxHeight">13dip</item>
      <item name="android:thumb">@drawable/restaurapptheme_scrubber_control_selector_holo_light</item>
      <item name="android:thumbOffset">16dip</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
  </style>

  <style name="RatingBarRestaurAppTheme" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/restaurapptheme_ratingbar_full_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/restaurapptheme_ratingbar_full_holo_light</item>
  </style>

  <style name="RatingBarBigRestaurAppTheme" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/restaurapptheme_ratingbar_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/restaurapptheme_ratingbar_holo_light</item>
      <item name="android:minHeight">35dip</item>
      <item name="android:maxHeight">35dip</item>
  </style>

  <style name="RatingBarSmallRestaurAppTheme" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/restaurapptheme_ratingbar_small_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/restaurapptheme_ratingbar_small_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="ToggleRestaurAppTheme" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/restaurapptheme_btn_toggle_holo_light</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ListViewRestaurAppTheme" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/restaurapptheme_list_selector_holo_light</item>
  </style>

  <style name="ListViewRestaurAppTheme.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/restaurapptheme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerItemRestaurAppTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">#000000</item>
  </style>

</resources>